{
  "name": "gym",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:pricing": "playwright test tests/e2e/pricing-production-ready.spec.ts tests/e2e/pricing-authenticated.spec.ts tests/e2e/pricing-api.spec.ts",
    "test:e2e:pricing:safe": "playwright test tests/e2e/pricing-production-ready.spec.ts",
    "test:e2e:pricing:complete": "playwright test tests/e2e/pricing-complete.spec.ts tests/e2e/pricing-production-ready.spec.ts tests/e2e/pricing-authenticated.spec.ts tests/e2e/pricing-api.spec.ts",
    "test:e2e:pricing:all": "playwright test tests/e2e/pricing-complete.spec.ts",
    "test:all": "npm run test && npm run test:e2e",
    "test:presentation": "npm run test:coverage && npm run test:e2e -- --reporter=html",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:push:force": "drizzle-kit push --force",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun scripts/create-admin.ts",
    "db:seed:admin": "bun scripts/seed-admin.ts",
    "db:seed:nationalities": "bun scripts/seed-nationalities.ts",
    "db:seed:test-members": "bun scripts/seed-1000-members-presentation.ts",
    "db:test-data": "psql $DATABASE_URL -f tests/data/test-scenarios.sql",
    "db:drop": "psql postgres://postgres:postgres@127.0.0.1:5432/postgres -c \"SELECT pg_terminate_backend(pid) FROM pg_stat_activity WHERE datname = 'podium_gym';\" && psql postgres://postgres:postgres@127.0.0.1:5432/postgres -c \"DROP DATABASE IF EXISTS podium_gym;\" && psql postgres://postgres:postgres@127.0.0.1:5432/postgres -c \"CREATE DATABASE podium_gym;\"",
    "db:setup": "npm run db:push:force && npm run db:seed:test-members",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:down:clean": "docker compose down -v && docker volume prune -f",
    "docker:up:db:clean:optimized": "docker compose down -v && docker volume prune -f && docker compose up -d && sleep 5 && npm run db:drop && npm run db:push:force && DATABASE_URL=\"postgres://postgres:postgres@127.0.0.1:5432/podium_gym\" bun scripts/seed-with-new-pricing.ts && DATABASE_URL=\"postgres://postgres:postgres@127.0.0.1:5432/podium_gym\" bun scripts/seed-1000-members-presentation.ts"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.10.0",
    "@neondatabase/serverless": "^1.0.1",
    "@next/third-parties": "^15.5.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@types/jspdf": "^1.3.3",
    "@vercel/analytics": "^1.4.1",
    "axios": "^1.12.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv-flow": "^4.1.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "embla-carousel-autoplay": "^8.5.1",
    "embla-carousel-react": "^8.5.1",
    "framer-motion": "^11.15.0",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "libphonenumber-js": "^1.12.17",
    "lucide-react": "^0.469.0",
    "next": "^15.5.3",
    "next-auth": "^5.0.0-beta.29",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-intersection-observer": "^9.14.1",
    "react-phone-number-input": "^3.4.12",
    "swr": "^2.3.2",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^4.1.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
