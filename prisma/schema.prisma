datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ClassSession {
  id            Int      @id @default(autoincrement())
  teacher       String
  classname     String
  startDatetime DateTime
  endDatetime   DateTime
  createdAt     DateTime @default(now())
}
